class Solution {
public:
    bool isValid(string s) {
        stack<char>st;
        for(char c : s){
            if(c=='['||c=='{'||c=='('){
                st.push(c);
            }
            else{
                if(st.empty()){
                    return false;
                }
                else{
                    char top1 = st.top();
                    st.pop();
                    if(c==']'&&top1!='['||c=='}'&&top1!='{'||c==')'&&top1!='('){
                        return false;
                    }
                }
            }
        }
        return st.empty();
    }
};

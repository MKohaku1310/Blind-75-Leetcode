class Solution {
public:
    vector<int> countBits(int n) {
        vector<int>ans(n+1);
        for(int i=0;i<=n;i++){
            int d = i;
            int cnt = 0;
            while(d>0){
                if(d%2==1) cnt++;
                d/=2;
            }
            ans[i]=cnt;
        }
        return ans;
    }
};
